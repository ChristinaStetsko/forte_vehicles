        <fullName>Plate_number_contain_or_starts_with_TX</fullName>
        <active>true</active>
        <description>Plate number should contain or start with &quot;TX&quot;</description>
        <errorConditionFormula>OR(
AND(
NOT(CONTAINS( Plate_Number__c , &quot;TX&quot;))
),

AND(
NOT(BEGINS(Plate_Number__c, &quot;TX&quot;))
)
)</errorConditionFormula>
